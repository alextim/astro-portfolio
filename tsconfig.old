{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2020",
    "module": "es2020",
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
     "paths": {
      "@/components/*": ["src/components/*"],
      "@/config/*": ["src/config/*"],
      "@/api/*": ["src/api/*"],
      "@/cms/*": ["src/cms/*"],
      "@/lib/*": ["src/lib/*"],
      "@/helpers/*": ["src/helpers/*"],
      "@/layouts/*": ["src/layouts/*"],
      "@/astro-eleventy-img/*": ["src/lib/astro-eleventy-img/index.ts"]
    }
  }
}
