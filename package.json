    "astro": "^0.25.4",
    "astro-icon": "^0.6.1",
    "astro-imagetools": "^0.4.1",
    "autoprefixer": "^10.4.4",
    "cssnano": "^5.1.5",
    "datauri": "^4.1.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
