{
  "name": "astro-portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier -w  --plugin-search-dir=. .",
    "lint": "eslint . --ext .ts,.js",
